{
  "name": "react-vscroller",
  "version": "0.1.0",
  "description": "React component which enables virtual rendering of collections",
  "source": "src/index.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "browser": "dist/react-vscroller.js",
  "files": [
    "*.md",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jrsearles/react-vscroller.git"
  },
  "bugs": {
    "url": "https://github.com/jrsearles/react-vscroller/issues"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production rollup -c",
    "types": "tsc --declarationDir dist/types --emitDeclarationOnly --declaration true --project tsconfig.build.json ",
    "format": "prettier --write --ignore-unknown \"**/*\"",
    "start": "yarn run storybook",
    "storybook": "start-storybook",
    "build-storybook": "build-storybook -o docs-build"
  },
  "keywords": [
    "react",
    "reactjs",
    "react-component",
    "intersection-observer",
    "virtualization"
  ],
  "author": "Joshua Searles",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@storybook/addon-knobs": "^6.1.10",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.1.10",
    "@types/faker": "^5.1.5",
    "@types/jest": "^25.1.2",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/resize-observer-browser": "^0.1.4",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "faker": "^5.1.0",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rollup": "^2.34.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.5"
  }
}
